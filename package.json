{
  "name": "SeriousTea",
  "private": true,
  "version": "0.0.1",
  "description": "Human+Nature webiste",
  "main": "index.js",
  "license": "MIT",
  "repository": "https://github.com/jwrigh26/aberdeen",
  "scripts": {
    "start-alpha": "npm run write-meta && webpack-dev-server --config webpack.config.js --mode development --devtool=source-map --env.devServerIndex=index.html --env.devToken=foo",
    "start-from-build": "webpack-dev-server --config webpack.config.js --mode production",
    "build": " npm run write-meta && webpack --config webpack.config.js --mode production",
    "preserve": "npm run build",
    "serve": "cross-env NODE_ENV=production firebase serve --only functions,hosting",
    "predeploy": "rimraf dist/ && npm run build",
    "deploy": "firebase deploy --only hosting",
    "deploy:funcs": "firebase deploy --debug --only functions",
    "test": "jest",
    "lint": "eslint --max-warnings 0 webpack.config.js \"src/**/*.js\"",
    "check-formatted": "prettier -c package.json .babelrc webpack.config.js \"src/**/!(mock)*.{js,scss}\"",
    "format": "prettier --write package.json .babelrc webpack.config.js \"src/**/!(mock)*.{js,scss}\"",
    "format-file": "prettier --write",
    "is-good": "npm run lint && npm run check-formatted && npm test",
    "dev:analyze": "npm run dev -- --env.addons=bundleanalyzer",
    "build:analyze": "npm run build -- --env.addons=bundleanalyzer",
    "write-meta": "node scripts/writeArticleMeta.js"
  },
  "browser": {
    "fs": false
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.{js,jsx}"
    ],
    "coverageDirectory": "./coverage",
    "coveragePathIgnorePatterns": [
      "/node_modules",
      "/coverage",
      "/config",
      "webpack.config.js",
      "/src/index.js",
      "/src/modules/palette"
    ],
    "moduleFileExtensions": [
      "",
      "js",
      "jsx"
    ],
    "modulePaths": [
      "helpers",
      "src",
      "styles",
      "modules"
    ],
    "moduleNameMapper": {
      "^helpers(.*)$": "<rootDir>/src/helpers$1",
      "^modules(.*)$": "<rootDir>/src/modules$1",
      "^styles(.*)$": "<rootDir>/src/styles$1",
      "^src(.*)$": "<rootDir>/src$1",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|scss|sass|less)$": "identity-obj-proxy"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/plugin-transform-modules-commonjs": "^7.15.4",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-react": "^7.14.5",
    "@webpack-cli/serve": "^1.5.2",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-import": "^1.13.3",
    "bluebird": "^3.7.2",
    "case-sensitive-paths-webpack-plugin": "^2.3.0",
    "clean-webpack-plugin": "^3.0.0",
    "compression-webpack-plugin": "^6.0.2",
    "css-loader": "^5.2.7",
    "dotenv-webpack": "^6.0.4",
    "eslint": "^7.32.0",
    "eslint-plugin-react": "^7.26.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-webpack-plugin": "^2.5.4",
    "favicons": "^6.2.2",
    "favicons-webpack-plugin": "^4.0.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^4.5.0",
    "jest": "^26.6.3",
    "line-reader": "^0.4.0",
    "mini-css-extract-plugin": "^1.6.2",
    "node-sass": "^5.0.0",
    "path": "^0.12.7",
    "prettier": "^2.4.1",
    "redux-devtools-extension": "^2.13.9",
    "sass-loader": "^10.2.0",
    "sinon": "^9.2.4",
    "style-loader": "^1.2.1",
    "url-loader": "^4.1.0",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.15.4",
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.60",
    "@reduxjs/toolkit": "^1.6.1",
    "axios": "^0.21.4",
    "classnames": "^2.3.1",
    "clsx": "^1.1.1",
    "d3": "^6.7.0",
    "date-fns": "^2.24.0",
    "dompurify": "^2.3.3",
    "emailjs-com": "^2.6.4",
    "firebase": "^8.10.0",
    "firebase-admin": "^9.11.1",
    "firebase-functions": "^3.15.7",
    "formik": "^2.2.9",
    "js-cookie": "^2.2.1",
    "lodash": "^4.17.21",
    "prop-types": "^15.7.2",
    "query-string": "^6.14.1",
    "ramda": "^0.27.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.15.4",
    "react-image": "^4.0.3",
    "react-redux": "^7.2.5",
    "react-router": "^5.2.1",
    "react-router-dom": "^5.3.0",
    "react-visibility-sensor": "^5.1.1",
    "uuid": "^8.3.2",
    "yup": "^0.32.9"
  }
}
